<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.simakmu.simakmu.ui.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.simakmu.simakmu.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.simakmu.simakmu.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_setupFragment"
            app:destination="@id/setupFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_transaksiFragment"
            app:destination="@id/transaksiFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_iuranFragment"
            app:destination="@id/iuranFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_analisaFragment"
            app:destination="@id/analisaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_keuanganFragment"
            app:destination="@id/keuanganFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/setupFragment"
        android:name="com.simakmu.simakmu.ui.setup.SetupFragment"
        android:label="fragment_setup"
        tools:layout="@layout/fragment_setup" >
        <action
            android:id="@+id/action_setupFragment_to_analisaFragment"
            app:destination="@id/analisaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_setupFragment_to_yayasanFragment"
            app:destination="@id/yayasanFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/transaksiFragment"
        android:name="com.simakmu.simakmu.ui.transaksi.TransaksiFragment"
        android:label="fragment_transaksi"
        tools:layout="@layout/fragment_transaksi" />
    <fragment
        android:id="@+id/iuranFragment"
        android:name="com.simakmu.simakmu.ui.iuran.IuranFragment"
        android:label="fragment_iuran"
        tools:layout="@layout/fragment_iuran" />
    <fragment
        android:id="@+id/analisaFragment"
        android:name="com.simakmu.simakmu.ui.analisa.AnalisaFragment"
        android:label="fragment_analisa"
        tools:layout="@layout/fragment_analisa" />
    <fragment
        android:id="@+id/yayasanFragment"
        android:name="com.simakmu.simakmu.ui.yayasan.YayasanFragment"
        android:label="fragment_yayasan"
        tools:layout="@layout/fragment_yayasan" >
        <action
            android:id="@+id/action_yayasanFragment_to_cabangFragment"
            app:destination="@id/cabangFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_yayasanFragment_to_lembagaFragment"
            app:destination="@id/lembagaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/cabangFragment"
        android:name="com.simakmu.simakmu.ui.yayasan.cabang.CabangFragment"
        android:label="fragment_cabang"
        tools:layout="@layout/fragment_cabang" >
        <action
            android:id="@+id/action_cabangFragment_to_ubcabFragment"
            app:destination="@id/ubcabFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/ubcabFragment"
        android:name="com.simakmu.simakmu.ui.yayasan.cabang.ubcab.UbcabFragment"
        android:label="fragment_ubcab"
        tools:layout="@layout/fragment_ubcab" >
        <action
            android:id="@+id/action_ubcabFragment_to_cabangFragment"
            app:destination="@id/cabangFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/cabangFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="cabang"
            app:argType="com.simakmu.simakmu.data.db.entities.Cabang" />
    </fragment>
    <fragment
        android:id="@+id/lembagaFragment"
        android:name="com.simakmu.simakmu.ui.yayasan.lembaga.LembagaFragment"
        android:label="fragment_lembaga"
        tools:layout="@layout/fragment_lembaga" >
        <action
            android:id="@+id/action_lembagaFragment_to_ublemFragment"
            app:destination="@id/ublemFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/ublemFragment"
        android:name="com.simakmu.simakmu.ui.yayasan.lembaga.ublem.UblemFragment"
        android:label="fragment_ublem"
        tools:layout="@layout/fragment_ublem" >
        <action
            android:id="@+id/action_ublemFragment_to_lembagaFragment"
            app:destination="@id/lembagaFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/lembagaFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="lembaga"
            app:argType="com.simakmu.simakmu.data.db.entities.Lembaga" />
    </fragment>
    <fragment
        android:id="@+id/keuanganFragment"
        android:name="com.simakmu.simakmu.ui.keuangan.KeuanganFragment"
        android:label="fragment_keuangan"
        tools:layout="@layout/fragment_keuangan" />
</navigation>